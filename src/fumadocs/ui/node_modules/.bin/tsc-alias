#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/tsc-alias@1.8.16/node_modules/tsc-alias/dist/bin/node_modules:/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/tsc-alias@1.8.16/node_modules/tsc-alias/dist/node_modules:/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/tsc-alias@1.8.16/node_modules/tsc-alias/node_modules:/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/tsc-alias@1.8.16/node_modules:/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/tsc-alias@1.8.16/node_modules/tsc-alias/dist/bin/node_modules:/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/tsc-alias@1.8.16/node_modules/tsc-alias/dist/node_modules:/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/tsc-alias@1.8.16/node_modules/tsc-alias/node_modules:/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/tsc-alias@1.8.16/node_modules:/home/tom/www2026/8_server/vercel/mydoc.site/src/fumadocs/ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsc-alias/dist/bin/index.js" "$@"
else
  exec node  "$basedir/../tsc-alias/dist/bin/index.js" "$@"
fi
