#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/@mixedbread+sdk@0.25.0/node_modules/@mixedbread/sdk/bin/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/@mixedbread+sdk@0.25.0/node_modules/@mixedbread/sdk/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/@mixedbread+sdk@0.25.0/node_modules/@mixedbread/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/@mixedbread+sdk@0.25.0/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/@mixedbread+sdk@0.25.0/node_modules/@mixedbread/sdk/bin/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/@mixedbread+sdk@0.25.0/node_modules/@mixedbread/sdk/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/@mixedbread+sdk@0.25.0/node_modules/@mixedbread/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/@mixedbread+sdk@0.25.0/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mixedbread/sdk/bin/cli" "$@"
else
  exec node  "$basedir/../@mixedbread/sdk/bin/cli" "$@"
fi
