#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/katex@0.16.22/node_modules/katex/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/katex@0.16.22/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/katex@0.16.22/node_modules/katex/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/katex@0.16.22/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../katex/cli.js" "$@"
else
  exec node  "$basedir/../katex/cli.js" "$@"
fi
