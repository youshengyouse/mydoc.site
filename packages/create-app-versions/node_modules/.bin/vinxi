#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/vinxi@0.5.8_@netlify+blobs@9.1.2_@types+node@24.3.0_db0@0.3.2_ioredis@5.7.0_jiti@2.5.1_lightn_zhabn5aenjhmmr4sww6xtvmoru/node_modules/vinxi/bin/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/vinxi@0.5.8_@netlify+blobs@9.1.2_@types+node@24.3.0_db0@0.3.2_ioredis@5.7.0_jiti@2.5.1_lightn_zhabn5aenjhmmr4sww6xtvmoru/node_modules/vinxi/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/vinxi@0.5.8_@netlify+blobs@9.1.2_@types+node@24.3.0_db0@0.3.2_ioredis@5.7.0_jiti@2.5.1_lightn_zhabn5aenjhmmr4sww6xtvmoru/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/vinxi@0.5.8_@netlify+blobs@9.1.2_@types+node@24.3.0_db0@0.3.2_ioredis@5.7.0_jiti@2.5.1_lightn_zhabn5aenjhmmr4sww6xtvmoru/node_modules/vinxi/bin/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/vinxi@0.5.8_@netlify+blobs@9.1.2_@types+node@24.3.0_db0@0.3.2_ioredis@5.7.0_jiti@2.5.1_lightn_zhabn5aenjhmmr4sww6xtvmoru/node_modules/vinxi/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/vinxi@0.5.8_@netlify+blobs@9.1.2_@types+node@24.3.0_db0@0.3.2_ioredis@5.7.0_jiti@2.5.1_lightn_zhabn5aenjhmmr4sww6xtvmoru/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vinxi/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../vinxi/bin/cli.mjs" "$@"
fi
