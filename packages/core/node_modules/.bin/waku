#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/waku@0.25.0_@swc+helpers@0.5.17_@types+node@24.3.0_@types+react@19.1.10_jiti@2.5.1_lightningc_zumu2lggq5c6hqq4oom4nccd3e/node_modules/waku/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/waku@0.25.0_@swc+helpers@0.5.17_@types+node@24.3.0_@types+react@19.1.10_jiti@2.5.1_lightningc_zumu2lggq5c6hqq4oom4nccd3e/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/waku@0.25.0_@swc+helpers@0.5.17_@types+node@24.3.0_@types+react@19.1.10_jiti@2.5.1_lightningc_zumu2lggq5c6hqq4oom4nccd3e/node_modules/waku/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/waku@0.25.0_@swc+helpers@0.5.17_@types+node@24.3.0_@types+react@19.1.10_jiti@2.5.1_lightningc_zumu2lggq5c6hqq4oom4nccd3e/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../waku/cli.js" "$@"
else
  exec node  "$basedir/../waku/cli.js" "$@"
fi
