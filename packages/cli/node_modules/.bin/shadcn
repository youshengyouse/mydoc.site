#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/shadcn@2.10.0_@types+node@24.3.0_typescript@5.9.2/node_modules/shadcn/dist/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/shadcn@2.10.0_@types+node@24.3.0_typescript@5.9.2/node_modules/shadcn/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/shadcn@2.10.0_@types+node@24.3.0_typescript@5.9.2/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/shadcn@2.10.0_@types+node@24.3.0_typescript@5.9.2/node_modules/shadcn/dist/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/shadcn@2.10.0_@types+node@24.3.0_typescript@5.9.2/node_modules/shadcn/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/shadcn@2.10.0_@types+node@24.3.0_typescript@5.9.2/node_modules:/home/tom/www2026/8_server/vercel/fumadocs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shadcn/dist/index.js" "$@"
else
  exec node  "$basedir/../shadcn/dist/index.js" "$@"
fi
